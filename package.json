  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": "20.6.1"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^2.2.1",
